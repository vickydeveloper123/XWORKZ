package com.LambdaExpressions;

interface Runnable{
	public String say(String name);
}

public class SingleParamter {
	public static void main(String[] args) {
		
		
		Runnable r=(name)->
		{
			return "Hello Darling"+name;
		};
		
		System.out.println("Welcome "+r.say("Shwetha My Frined"));
	}
	
	

  
	

}
