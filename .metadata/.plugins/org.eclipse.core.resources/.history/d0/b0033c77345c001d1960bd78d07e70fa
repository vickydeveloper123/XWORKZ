package com.LambdaExpressions;

/*
 * public interface Test {
 * 
 * public void test(Integer a,Integer b);
 * 
 * }
 * 
 * class Test1{
 * 
 * public static void Testing(Test c,Integer a,Integer b) {
 * 
 * c.test(a, b);
 * 
 * }
 * 
 * public static void main(String[] args) {
 * 
 * Testing((a,b)->System.out.println(a+" "+b),2 ,3);
 * 
 * 
 * 
 * 
 * }
 * 
 * }
 */

interface Test3 {
    
    void print(Integer p1, Integer p2);
}
 
class GfG {
    
    static void fun(Test3 t, Integer p1, Integer p2)
    {
        t.print(p1, p2);
    }
    public static void main(String[] args)
    {
        
        fun((p1, p2)
                -> System.out.println(p1 + " " + p2),
            10, 20);
    }
}
