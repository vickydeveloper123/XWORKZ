
interface Drawable{  
    public void draw();  
}  
  
public class GFG {  
    public static void main(String[] args) {  
        int age=30;
        String name="Shashi";
          
        //with lambda  
        Drawable d2=()->{  
            System.out.println("Drawing "+"Gender Age ="+age+"\n"+"GenderName =="+ name);  
        };  
        d2.draw();  
    }  
}  
